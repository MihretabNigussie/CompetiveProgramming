class Solution:
    def pivotIndex(self, nums: List[int]) -> int:
        prefix = []
        postfix = []
        index = 0
        mySum = 0
        while index < len(nums):
            prefix.append(mySum)
            mySum+=nums[index]
            index+=1
        index = len(nums)-1
        postfix = [0]* len(nums)
        mySum = 0
        while index > -1:
            postfix[index] = mySum
            mySum += nums[index]
            index-=1
        output = -1
        index = 0
       
        while index < len(nums):
            if prefix[index] == postfix[index]:
                return index
            index+=1
        return -1
