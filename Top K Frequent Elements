class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        hashmap = Counter(nums)
        output = []
        for num in hashmap:
            output.append([num,hashmap[num]])
        output.sort(key = lambda x : x[1],reverse=True)
        final = []
        for i in range(k):
            final.append(output[i][0])
        return final
        
