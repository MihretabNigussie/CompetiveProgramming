class Solution:
    def nextGreaterElement(self, nums1: List[int], nums2: List[int]) -> List[int]:
        hashmap = {i:n for n,i in enumerate(nums1)}
        
        stack = []
        output = [-1]* len(nums1)
        for n in nums2:
            
            while stack and n > stack[-1]: 
                output[hashmap[stack.pop()]] = n
            if n in hashmap:
                stack.append(n)
        return output
        
        
# OR ELSE

def nextGreaterElement(nums1, nums2):
    lst = []
    for i in nums1:
        n = nums2.index(i)
        if i != nums2[-1]:
            for j in nums2[n+1:]:
                if i < j:
                    lst.append(j)
                    break
            else:
                lst.append(-1)
        else:
            lst.append(-1)
    return lst

