class Solution:
    def nextGreaterElement(self, nums1: List[int], nums2: List[int]) -> List[int]:
        hashmap = {i:n for n,i in enumerate(nums1)}
        
        stack = []
        output = [-1]* len(nums1)
        for n in nums2:
            
            while stack and n > stack[-1]: 
                output[hashmap[stack.pop()]] = n
            if n in hashmap:
                stack.append(n)
        return output
